+        bool did_work = false;
+        absl::optional<DiscoveryResponse> response;
+        {
+          grpc_core::MutexLock lock(&parent_->ads_mu_);
+          if (stream_closed || parent_->ads_done_) break;
+          if (!requests.empty()) {
