+      db_->GetTailingIteratorPair(options_, cfd_, &version_number_);
+  assert(iters.first && iters.second);
+  mutable_.reset(iters.first);
+  immutable_.reset(iters.second);
+  current_ = nullptr;
+  is_prev_set_ = false;
